mcdp {
    provides trees [Nat]
    provides sidewalks [Nat]
    provides benches [Nat]
    
    requires initial_budget [$]
    requires community_approval [dimensionless]

    required initial_budget * (1 + (required community_approval / 100)) >= (provided trees * 1000 $) + (provided sidewalks * 400 $) + (provided benches * 2000 $)
}