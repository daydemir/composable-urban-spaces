mcdp {
    provides community_approval [dimensionless]

    requires greenery [dimensionless]
    requires accessibility [dimensionless]
    requires sustainability [dimensionless]
        required affordability [$]
    
    cost_dislike = 1 [dimensionless]
    affordability = (provided acceptable_cost_yearly * cost_dislike)
    effects =  + required sustainability
    quality = required greenery * required accessibility

    provided community_approval <= quality + effects
}
